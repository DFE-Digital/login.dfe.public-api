{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Public Api",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/../.env",
            "env": {
                "settings": "${workspaceFolder}/config/local-config.json",
                "NODE_TLS_REJECT_UNAUTHORIZED": "0"
            },
            "program": "${workspaceFolder}/src/index.js",
            "outputCapture": "std"
        }
    ]
}
